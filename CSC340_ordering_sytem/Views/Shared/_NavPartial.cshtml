<!-- Header
          ============================================= -->
<header id="header" class="header-transparent">
    <div class="container">
        <div class="row">
            <div id="main-menu-trigger"><i class="fa fa-bars"></i></div>
            <!-- Logo
                        ============================================= -->
            <div id="logo">
                <a href="#" class="light-logo">
                    <img src="@Url.Content("~/Content/Assets/logo.png")" alt="Logo"/>
                </a>
                <a href="#" class="dark-logo">
                    <img src="@Url.Content("~/Content/Assets/logo_dark.png")" alt="Logo"/>
                </a>
            </div>
            <!--End #logo  -->
            <!-- Primary Navigation
                        ============================================= -->
            <nav id="main-menu" class="dark">
                @Html.Partial("_NavLinks")
            </nav>
            <!-- #main-menu end -->
        </div>
    </div>
</header>
<!-- End Header Transparent -->